<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  
 
  
  <servlet>
  	<servlet-name>Login</servlet-name>
  	<display-name>Login</display-name>
  	<description></description>
  	<servlet-class>com.amdocs.web.LoginController</servlet-class>
  </servlet>

  <servlet>
  	<servlet-name>CoursesController</servlet-name>
  	<display-name>CoursesController</display-name>
  	<description></description>
  	<servlet-class>com.amdocs.web.EnrollCoursesController</servlet-class>
  </servlet>
  
  <servlet>
  	<servlet-name>FeedbackController</servlet-name>
  	<display-name>FeedbackController</display-name>
  	<description></description>
  	<servlet-class>com.amdocs.web.FeedbackController</servlet-class>
  </servlet>
  
  <servlet>
  	<servlet-name>RegisterController</servlet-name>
  	<display-name>RegisterController</display-name>
  	<description></description>
  	<servlet-class>com.amdocs.web.RegisterController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Logout</servlet-name>
  	<display-name>Logout</display-name>
  	<description></description>
  	<servlet-class>com.amdocs.web.LogoutController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ContactUsController</servlet-name>
  	<display-name>ContactUsController</display-name>
  	<description></description>
  	<servlet-class>com.amdocs.web.ContactUsController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>EnrolledCoursesController</servlet-name>
  	<display-name>EnrolledCoursesController</display-name>
  	<description></description>
  	<servlet-class>com.amdocs.web.EnrolledCoursesController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>EnrollCoursesUpdateController</servlet-name>
  	<display-name>EnrollCoursesUpdateController</display-name>
  	<description></description>
  	<servlet-class>com.amdocs.web.EnrollCoursesUpdateController</servlet-class>
  </servlet>
  
  
  <servlet-mapping>
  	<servlet-name>Login</servlet-name>
  	<url-pattern>/login</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
  	<servlet-name>CoursesController</servlet-name>
  	<url-pattern>/enrollCourse</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
  	<servlet-name>FeedbackController</servlet-name>
  	<url-pattern>/feedback</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
  	<servlet-name>RegisterController</servlet-name>
  	<url-pattern>/register</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
  	<servlet-name>Logout</servlet-name>
  	<url-pattern>/logout</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
  	<servlet-name>ContactUsController</servlet-name>
  	<url-pattern>/contactUs</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
  	<servlet-name>EnrolledCoursesController</servlet-name>
  	<url-pattern>/enrolledCourses</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>EnrollCoursesUpdateController</servlet-name>
  	<url-pattern>/enrollCoursesUpdate</url-pattern>
  </servlet-mapping>
  
  <welcome-file-list>
    <welcome-file>login.jsp</welcome-file>
</welcome-file-list>

</web-app>
